{
  "name": "tecnodesignc/media-module",
  "type": "encore-module",
  "description": "Media module for EncoreCMS. Handles the media library.",
  "keywords": [
    "EncoreCMS",
    "media",
    "files",
    "library",
    "thumbnails",
    "filters"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcos Rativa",
      "email": "marcos21.009@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@tecnodesign.com.co",
    "issues": "https://github.com/tecnodesignc/Platform/issues",
    "source": "https://github.com/tecnodesignc/Media"
  },
  "require": {
    "php": "^7.1.3",
    "composer/installers": "^1.0",
