    "tecnodesignc/tag-module": "^1.0",
    "guzzlehttp/psr7": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "3.6.*",
    "league/flysystem-aws-s3-v3": "~1.0",
